#!/usr/bin/make -f

export MONO_DISABLE_SHM = 1

override_dh_auto_build:
	nant -nologo -q build
override_dh_install:
	nant -nologo -q -D:DESTDIR=$(CURDIR)/debian/tmp  install
	dh_install
override_dh_fixperms:
	dh_fixperms
	dh_clifixperms
override_dh_shlibdeps:
	dh_shlibdeps
	dh_clideps -i

%:
	dh $@
